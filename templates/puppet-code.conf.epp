{
    "service-url": "https://<%= $pe_server_certname %>:8170/code-manager",
    <% if $kernel == 'windows' { -%>
    "cacert": "C:\\ProgramData\\PuppetLabs\\puppet\\etc\\ssl\\certs\\<%= $certname %>",
    <% } else { -%>
    "cacert": "/etc/puppetlabs/puppet/ssl/certs/<%= $certname %>",
    <% } -%>
    "token-file": "<%= $access_token_path %>"
}
